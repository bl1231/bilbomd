{
  "name": "@bilbomd/ui",
  "version": "1.24.5",
  "description": "Frontend Single Page Web Application for BilboMD",
  "private": true,
  "type": "module",
  "homepage": "https://bilbomd.bl1231.als.lbl.gov",
  "author": {
    "name": "Scott Classen",
    "email": "sclassen@lbl.gov"
  },
  "contributors": [
    {
      "name": "Kai Classen",
      "email": "kai.ko.classen@icloud.com"
    },
    {
      "name": "Shreyas Prabhakar",
      "email": "ShreyasPrabhakar@lbl.gov"
    }
  ],
  "scripts": {
    "clean": "rimraf build/*",
    "dev": "GIT_HASH=$(git rev-parse --short HEAD) vite",
    "prebuild": "tsc && cp ../../LICENSE.txt public/LICENSE.txt",
    "vite-build": "vite build",
    "lint": "eslint",
    "build": "npm-run-all clean vite-build",
    "build:ci": "npm-run-all clean lint vite-build",
    "preview": "vite preview",
    "optimize": "vite optimize",
    "test": "vitest --run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "related": "vitest related"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "vitest related --run",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/colors": "^8.0.0",
    "@bilbomd/mongodb-schema": "workspace:*",
    "@bilbomd/bilbomd-types": "workspace:*",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@mui/utils": "^7.3.3",
    "@mui/x-data-grid": "^8.16.0",
    "@reduxjs/toolkit": "^2.9.2",
    "axios": "^1.13.1",
    "axios-retry": "^4.5.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "form-data": "^4.0.4",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "molstar": "^5.2.0",
    "notistack": "^3.0.2",
    "prop-types": "^15.8.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.9.5",
    "react-smooth": "^4.0.4",
    "react-spinners": "^0.17.0",
    "recharts": "^3.3.0",
    "yup": "^1.7.1"
  },
  "overrides": {
    "react-is": "^19.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "sass-embedded": "^1.93.3"
  }
}
