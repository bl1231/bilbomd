    "url": "git+https://github.com/bl1231/bilbomd.git",
    "directory": "packages/mongodb-schema"
  },
  "author": "Scott Classen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bl1231/bilbomd/issues"
  },
  "homepage": "https://github.com/bl1231/bilbomd/tree/main/packages/mongodb-schema#readme",
  "sideEffects": false,
  "dependencies": {
    "mongoose": "^8.19.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
